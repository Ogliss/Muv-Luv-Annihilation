<Defs>

  <PawnKindDef Name="MLB_MILIT_NPC_Kind" ParentName="MLB_MILIT_Base_Kind" Abstract="true"> <!-- Base in PawnKinds_Player_Humanoids.xml -->
    <defaultFactionType>MLB_MILIT_Faction</defaultFactionType>
    <invNutrition>2.55</invNutrition>
    <combatPower>60</combatPower>
	<apparelTags>
		<li>MILIT_Clothing</li>
		<li>MILIT_Armour</li>
	</apparelTags>
	<apparelRequired>
        <!--li>MLB_MILIT_Apparel_Fatigues</li-->
	</apparelRequired>
  </PawnKindDef>
  

<!--=============================================== MILIT - NPC Faction ===============================================-->

  <PawnKindDef ParentName="MLB_MILIT_NPC_Kind"> <!-- Base in PawnKinds_Player_Humanoids.xml -->
    <defName>MLB_MILIT_Lieutenant</defName>
    <label>marine Lt</label>
    <chemicalAddictionChance>0.08</chemicalAddictionChance>
    <factionLeader>true</factionLeader>
    <combatPower>120</combatPower>
    <gearHealthRange>
      <min>0.4</min>
      <max>1.4</max>
    </gearHealthRange>
	<apparelTags>
		<li>MILIT_Clothing</li>
		<li>MILIT_Armour</li>
	</apparelTags>
	<apparelMoney>
		<min>5000</min>
		<max>10000</max>
    </apparelMoney>
    <weaponTags>
		<li>MILIT_</li>
	</weaponTags>
	<weaponMoney>
		<min>1000</min>
		<max>1000</max>
	</weaponMoney>
	<techHediffsMoney>
      <min>2000</min>
      <max>3000</max>
    </techHediffsMoney>
    <techHediffsTags>
      <li>Advanced</li>
      <li>AdvancedWeapon</li>
    </techHediffsTags>
    <techHediffsChance>0.40</techHediffsChance>
    <combatEnhancingDrugsChance>0.2</combatEnhancingDrugsChance>
    <combatEnhancingDrugsCount>
      <min>0</min>
      <max>2</max>
    </combatEnhancingDrugsCount>
	<backstoryCryptosleepCommonality>1</backstoryCryptosleepCommonality>
	<inventoryOptions>
      <skipChance>0.8</skipChance>
      <subOptionsChooseOne>
        <li>
          <thingDef>Silver</thingDef>
          <countRange>
            <min>50</min>
            <max>200</max>
          </countRange>
        </li>
        <li>
          <thingDef>MedicineIndustrial</thingDef>
          <countRange>
            <min>1</min>
            <max>1</max>
          </countRange>
        </li>
      </subOptionsChooseOne>
    </inventoryOptions>
	<apparelRequired>
	<!--
        <li>MLB_MILIT_Armour</li>
        <li>MLB_MILIT_Headgear_Cap</li>
	-->
	</apparelRequired>
  </PawnKindDef>
  
